{
    "name": "signnow/api-php-sdk",
    "license": "MIT",
    "description": "Library gives you ability to communicate with SignNow API",
    "type": "library",
    "keywords": ["php sdk", "api", "signnow"],
    "autoload": {
        "psr-4": {
            "SignNow\\Api\\": "src",
            "Examples\\": "examples/"
        }
    },
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "signnow/rest-entity-manager": "^1.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "codeception/codeception": "^4.1",
        "codeception/module-asserts": "^1.2",
        "mcustiel/phiremock-codeception-extension": "^2.0",
        "mcustiel/phiremock-server": "^1.0",
        "mcustiel/phiremock-client": "^1.2",
        "mcustiel/phiremock-codeception-module": "^1.1"
    },
    "autoload-dev": {
        "psr-4": {
            "SignNow\\Tests\\Functional\\": "tests/functional/",
            "SignNow\\Tests\\Unit\\": "tests/unit/"
        },
        "classmap": [
            "tests/_support"
        ]
    },
    "prefer-stable": true
}
